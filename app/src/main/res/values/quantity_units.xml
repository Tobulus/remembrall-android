<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Matching code of a quantity-unit is at the same position in the array 'quantity_units_codes'-->
    <string-array name="quantity_units">
        <item>@string/piece</item>
        <item>@string/liter</item>
        <item>@string/milli_liter</item>
        <item>@string/gram</item>
        <item>@string/kilo_gram</item>
    </string-array>
    <!-- These codes must match the codes used by the backend -->
    <string-array name="quantity_units_codes">
        <item>P</item>
        <item>L</item>
        <item>ML</item>
        <item>G</item>
        <item>KG</item>
    </string-array>
</resources>